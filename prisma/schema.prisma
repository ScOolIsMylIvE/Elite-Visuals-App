generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id    String @id @default(cuid())
  name  String
  users User[]
}

model User {
  id        String  @id @default(cuid())
  email     String  @unique
  firstName String
  lastName  String
  account   Account @relation(fields: [accountId], references: [id])
  accountId String
}
